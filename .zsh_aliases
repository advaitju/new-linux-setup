GA='git add .'
GC='git commit -m '
GCF='git add . && git commit -m \"Commit\" && git push'
GCH='git add . && git commit -m \"Commit\" && git push heroku main && git push'
GP='git push'
GSF='git stash && git pull && git push && git stash pop'

# Aliases
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

alias c='clear && tput reset'
alias lsa='ls -a'

alias t='tmux'
alias t0='tmux attach-session -t 0'
alias t1='tmux attach-session -t 1'
alias t2='tmux attach-session -t 2'
alias t3='tmux attach-session -t 3'
alias t4='tmux attach-session -t 4'
alias tls='tmux ls'

alias h='heroku'

alias g='git'
alias gs='git status'
alias ga=$GA
alias gc=$GC
alias gcf=$GCF
alias gch=$GCH
alias gp=$GP
alias gsf=$GSF
